    };

    var pageFrom = "ArticleList";
    var calpValue = CALP_VALUE[pageFrom];
    var adPlaceReport = "CAFE_ARTICLE_LIST";


    
    var cafeId = "28647568";
    var cafeUrl = "sunbaakim";
    var advertKeyword = "";
    var articleGdId = "";
    var articleId = "";
    var menuId = "";
    var title = "";
    var skinType = "WIDE_SKIN";
    var skinTone = "LIGHT";
    // old skin
    var oldSkinNoticeFontColor = Element.getCSS($('_noticeColor'), "color");

    var ADVERT_FONT_COLOR = {
        'WIDE_SKIN' : {
            'LIGHT' : {
                'DEFAULT_COLOR' : "rgb(34, 34, 34)", // #222222
                'HIGHTLIGHT_COLOR' : "rgb(7, 26, 200)" // #071AC8
            },
            'DARK' : {
                'DEFAULT_COLOR' : "rgb(177, 177, 177)", // #B1B1B1
                'HIGHTLIGHT_COLOR' : "rgb(255, 255, 255)" // #FFFFFF
            }
        },
        'OLD_SKIN' : {
            'LIGHT' : {
                'DEFAULT_COLOR' : "rgb(34, 34, 34)", // #222222
                'HIGHTLIGHT_COLOR' : oldSkinNoticeFontColor
            },
            'DARK' : {
                'DEFAULT_COLOR' : "rgb(177, 177, 177)", // #B1B1B1
                'HIGHTLIGHT_COLOR' : oldSkinNoticeFontColor
            }
        }
    };

    //generate uuid
    function uuidv4() {
        var uuid = "", i, random;
        for (i = 0; i < 32; i++) {
            random = Math.random() * 16 | 0;
            if (i == 8 || i == 12 || i == 16 || i == 20) {
                uuid += "-"
            }
            uuid += (i == 12 ? 4 : (i == 16 ? (random & 3 | 8) : random)).toString(16);
        }
        return uuid;
    }

    var deviceId = getDeviceId();
    var requestId = uuidv4();
    var spwp = spwp || {};
    spwp.cmd = spwp.cmd || [];
    spwp.cmd.push(function () {
