            enablePersistAd: false
        });

        if (pageFrom === "ArticleList") {
            menuId = "";
            title = "카페 가입기념 선바님 팬아트 그려봤어요!";
        } else if (pageFrom === "ArticleSearchList") {
            menuId = "";
            advertKeyword = "null";
            title = "";
        } else if (pageFrom === "ArticleRead") {
            title = "";
            articleGdId = "";
            articleId = "";
            menuId = "";
            advertKeyword = "";
        }

        var fontColorType = "";
        if (skinType === "" || skinTone === "") {
            fontColorType = ADVERT_FONT_COLOR['WIDE_SKIN']['LIGHT'];
        } else {
            fontColorType = ADVERT_FONT_COLOR[skinType][skinTone];
        }

        var adUnits = [{
            unitId: "pcweb_cafe",
            divId: "powerAd-div",

            extraOption: {
                calp: calpValue,
                mediaParams: {
                    styleDefault: fontColorType.DEFAULT_COLOR,
                    styleHighlight: fontColorType.HIGHTLIGHT_COLOR,
                    gdid: articleGdId,
                    pk: advertKeyword,
                    cas: cafeUrl,
                    cid: cafeId,
                    boardId: menuId,
                    pid: articleId,
                    mediaRequestId: requestId,
                    title: title
                }
            }
        }];

        var eventList = {};
        eventList[spwp.GfpAdEvent.Type.AD_LOAD] = function (adEvent) {
            oGroupSSPAdvertReporter.report(oGroupSSPAdvertReporter.REQUEST_PARAM.REPORT_TYPE.ACK, spwp.GroupSSP);
        };

        eventList[spwp.GfpAdErrorEvent.Type.AD_LOAD_FAILED] = function (adErrorEvent) {
            oGroupSSPAdvertReporter.report(oGroupSSPAdvertReporter.REQUEST_PARAM.REPORT_TYPE.NO_FILL, spwp.GroupSSP);
        };

        eventList[spwp.GfpAdEvent.Type.AD_IMPRESSION] = function (adEvent) {
            oGroupSSPAdvertReporter.report(oGroupSSPAdvertReporter.REQUEST_PARAM.REPORT_TYPE.IMP, spwp.GroupSSP);
            if (pageFrom === "ArticleRead") {
                try {
                    parent.resizeIframe('cafe_main');
                } catch (x) {}
            }
        };

        eventList[spwp.GfpAdEvent.Type.AD_CLICK] = function (adEvent) {
            oGroupSSPAdvertReporter.report(oGroupSSPAdvertReporter.REQUEST_PARAM.REPORT_TYPE.CLICK, spwp.GroupSSP);
        };

        /* SSP sdk call */
        spwp.addAdUnits(adUnits);
        spwp.setAdListener({eventList: eventList});
        spwp.requestAds({
            adUnitIds: ["pcweb_cafe"]
        });
    });

    var initSSpAd = function () {
        var daGroupSSPUrl = "https://ipada.cafe.naver.com";
        var daGroupSSPRequestParameter = {
            serviceId: 'cafe',
            platform: "PC_WEB",
            creativeType: oGroupSSPAdvertFinder.REQUEST_PARAM.CREATIVE_TYPE.SA,
            adPlacement: adPlaceReport,
            requestCount: 1,
            userKey: "ttobokk",
            deviceId: deviceId,
            requestId: requestId,
            language: navigator.language,
            os: oGroupSSPAdvertFinder.os(),
            cafeId: cafeId,
            postId: ""
        };

        oGroupSSPAdvertFinder.findWithDA(daGroupSSPUrl, daGroupSSPRequestParameter, "", spwp, null);
    };

    if (window.addEventListener) {
        window.addEventListener("load", initSSpAd, false);
    } else if (window.attachEvent) {
        window.attachEvent("onload", initSSpAd);
    } else {
        window.onload = initSSpAd;
    }
</script>
